{
	"compilerOptions": {
        "module": "esNext",
		"target": "es2021",
		"moduleResolution": "node",
		"strict": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
		"preserveConstEnums":true,
        "sourceMap": true,
        "rootDir": "./",
        "esModuleInterop": true,
        "resolveJsonModule": true
	}
}
